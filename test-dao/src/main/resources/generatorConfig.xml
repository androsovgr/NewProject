<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <context id="testContext" targetRuntime="MyBatis3">
        <jdbcConnection driverClass="org.postgresql.Driver"
                        connectionURL="jdbc:postgresql://localhost:5432/NIR" userId="postgres" password="">
        </jdbcConnection>
        <javaModelGenerator targetPackage="dao.model"
                            targetProject="src/main/java">
        </javaModelGenerator>
        <sqlMapGenerator targetPackage="dao.mapper"
                         targetProject="src/main/resources">
        </sqlMapGenerator>
        <javaClientGenerator type="XMLMAPPER"
                             targetPackage="dao.mapper" targetProject="src/main/java">
        </javaClientGenerator>
        <table tableName="people" domainObjectName="model.Model" schema="people" >
            <generatedKey column="id_person" sqlStatement="JDBC"  identity="true" />
        </table>
    </context>
</generatorConfiguration>